{
  "name": "@akashic/complete-audio",
  "version": "0.2.5",
  "description": "complement audio asset files",
  "main": "lib/complete.js",
  "scripts": {
    "prepublish": "rimraf ./lib && npm run build",
    "start": "npm run build && node bin/run",
    "build": "tsc -p ./",
    "lint": "npm run lint:ts && npm run lint:md",
    "lint:ts": "eslint src/*.ts src/**/*.ts --fix",
    "lint:md": "remark ./*.md --frail --no-stdout --rc-path ./.remarkrc",
    "test": "npm run test:ts && npm run lint",
    "test:ts": "npm run build && jest"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "bin": {
    "complete-audio": "./bin/run"
  },
  "files": [
    "bin",
    "lib"
  ],
  "devDependencies": {
    "@akashic/eslint-config": "^0.1.2",
    "@types/commander": "2.12.0",
    "@types/node": "14.14.26",
    "@types/jest": "~26.0.20",
    "@types/fluent-ffmpeg": "~2.1.16",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "eslint": "^7.18.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "jasmine": "^2.1.1",
    "istanbul": "^0.3.2",
    "jasmine-reporters": "~2.0.4",
    "rimraf": "^3.0.2",
    "typescript": "~4.1.2",
    "remark-cli": "~9.0.0",
    "remark-lint": "~8.0.0",
    "mock-fs": "^3.12.1",
    "jest": "~26.6.3"
  },
  "dependencies": {
    "commander": "^5.0.0",
    "fluent-ffmpeg": "^2.0.1"
  },
  "publishConfig": {
    "@akashic:registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akashic-games/complete-audio.git"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "testMatch": [
      "./**/*[sS]pec.js"
    ]
  }
}
