{
  "name": "@akashic/complete-audio",
  "version": "0.2.4",
  "description": "complement audio asset files",
  "main": "lib/complete.js",
  "scripts": {
    "prepublish": "rimraf ./lib && npm run install:typings && npm run build",
    "start": "npm run build && node bin/run",
    "install:typings": "typings install",
    "build": "tsc -p ./",
    "lint": "npm run lint:ts && npm run lint:md",
    "lint:ts": "tslint --project tsconfig.json -c tslint.json",
    "lint:md": "remark ./*.md --frail --no-stdout --rc-path ./.remarkrc",
    "test": "npm run test:ts && npm run lint",
    "test:ts": "npm run build && npm run test:jasmine",
    "test:jasmine": "istanbul cover --report text --report html --colors -i ./lib/main.node.js ./node_modules/jasmine/bin/jasmine.js"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "bin": {
    "complete-audio": "./bin/run"
  },
  "files": [
    "bin",
    "lib"
  ],
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~2.1.6",
    "typings": "^2.1.0",
    "tslint": "~5.4.3",
    "jasmine": "^2.1.1",
    "istanbul": "^0.3.2",
    "jasmine-reporters": "~2.0.4",
    "remark-cli": "~2.0.0",
    "remark-lint": "~5.0.1",
    "mdast": "^2.0.0",
    "mdast-lint": "^1.1.1",
    "mock-fs": "^3.12.1"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "fluent-ffmpeg": "^2.0.1"
  },
  "publishConfig": {
    "@akashic:registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akashic-games/complete-audio.git"
  }
}
